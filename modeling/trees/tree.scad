
  // the base square has size d
  module octahedron(d) {
    rotate(45) {
      linear_extrude(d/sqrt(2), scale=0)
      square(d, center=true);

      mirror([0,0,1])
      linear_extrude(d/sqrt(2), scale=0)
      square(d, center=true);
    }
  }

  module triangle(x, y) {
    mirror([0,0,1])
    linear_extrude(0.1, scale=0)
    polygon([
      [x, 0],
      [0, y/2],
      [0, -y/2]
    ]);
  };


